{
  "name": "create-fastify-ts",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "author": "",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch --inspect src/index.ts",
    "build": "tsup src/index.ts --format esm --sourcemap --dts --clean",
    "preview": "node --experimental-specifier-resolution=node --no-warnings dist/index.js",
    "prettify": "prettier --write src/ test/",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "vitest --run",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@mgcrea/fastify-request-logger": "^1.1.1",
    "@mgcrea/pino-pretty-compact": "^1.2.1",
    "fastify": "^4.18.0"
  },
  "devDependencies": {
    "@mgcrea/eslint-config-node": "^0.6.1",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node-lts": "^18.12.2",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^18.16.17",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0"
  }
}
