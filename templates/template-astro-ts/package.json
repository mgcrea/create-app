    "eslint": "^8.55.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier": "^3.1.0",
    "sharp": "^0.33.0",
    "typescript": "^5.3.3"
  }
}
